@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://github.com/sensorium/sensorium-plugins>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-thecloud.html> ;
        modgui:stylesheet <modgui/stylesheet-thecloud.css> ;
        modgui:screenshot <modgui/screenshot-thecloud.png> ;
        modgui:thumbnail <modgui/thumbnail-thecloud.png> ;
        modgui:brand "sensorium" ;
        modgui:label "thecloud" ;
        modgui:panel "0500" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "density" ;
            lv2:name "density" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "detune" ;
            lv2:name "detune" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "flow" ;
            lv2:name "flow" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "mix" ;
            lv2:name "mix" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "straight_delay" ;
            lv2:name "input delay" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "time" ;
            lv2:name "time" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "width" ;
            lv2:name "width" ;
        ], [
            lv2:index 7 ;
            lv2:symbol "env_type" ;
            lv2:name "envelope" ;
        ] ;
    ] .